    "Resizing",
    "Resizer",
    "postMessage",
    "content",
    "resize",
    "height",
    "autoheight",
    "auto-height",
    "iframe-auto-height",
    "height-iframe",
    "heightiframe",
    "width",
    "mutationObserver",
    "RWD",
    "responsive",
    "responsiveiframes",
    "responsive-iframes"
  ],
  "license": "MIT",
  "ignore": [
    "example",
    "test",
    "gruntfile.js",
    "*.md",
    "*.json"
  ],
  "dependencies": {},
  "devDependencies": {}
